{
  "name": "open-backend-nodejs",
  "version": "0.0.1",
  "description": "My open source backend code wrote with express.",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.js": "eslint --cache --fix"
  },
  "_moduleAliases": {
    "@root": "./",
    "@routes": "src/Routes",
    "@modules": "src/Modules",
    "@config": "src/Config"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npx mrm lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntoineMaillard06/open-backend-nodejs.git"
  },
  "author": "AntoineMaillard06",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/AntoineMaillard06/open-backend-nodejs/issues"
  },
  "homepage": "https://github.com/AntoineMaillard06/open-backend-nodejs#readme",
  "dependencies": {
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1"
  }
}
